<robot name="pendulum">

    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>

    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>

    <material name="gray">
        <color rgba="0.6 0.6 0.6 1"/>
    </material>

    <link name="world"/>
    <link name="base">
        <visual>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
            <material name="white"/>
        </visual>
        <inertial>
            <mass value="0"/>
            <inertia ixx="0"  ixy="0"  ixz="0" iyy="0" iyz="0" izz="0" />
        </inertial>
    </link>

    <link name="link_1">
        <visual>
            <geometry>
                <cylinder radius="0.01" length="0.8"/>
            </geometry>
            <origin xyz="0 0 0.4" rpy="0 0 0"/>
            <material name="gray"/>
        </visual>
        <inertial>
            <mass value="0"/>
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
    </link>

    <link name="link_2">
        <visual>
            <geometry>
                <cylinder radius="0.01" length="0.8"/>
            </geometry>
            <origin xyz="0 0 0.4" rpy="0 0 0"/>
            <material name="gray"/>
        </visual>
        <inertial>
            <mass value="0"/>
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
    </link>

    <link name="link_3">
        <visual>
            <geometry>
                <cylinder radius="0.01" length="0.8"/>
            </geometry>
            <origin xyz="0 0 0.4" rpy="0 0 0"/>
            <material name="gray"/>
        </visual>
        <inertial>
            <mass value="0"/>
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
    </link>

    <link name="link_eef">
        <visual>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <origin xyz="0 0 0.05" rpy="0 0 0" />
            <material name="black"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1"/>
            <inertia ixx="0"  ixy="0"  ixz="0" iyy="0" iyz="0" izz="0" />
        </inertial>
    </link>

    <link name="link_eef_2">
        <visual>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <origin xyz="0 0 0.05" rpy="0 0 0" />
            <material name="black"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1"/>
            <inertia ixx="0"  ixy="0"  ixz="0" iyy="0" iyz="0" izz="0" />
        </inertial>
    </link>

    <link name="link_eef_3">
        <visual>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <origin xyz="0 0 0.05" rpy="0 0 0" />
            <material name="black"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1"/>
            <inertia ixx="0"  ixy="0"  ixz="0" iyy="0" iyz="0" izz="0" />
        </inertial>
    </link>

    <joint name="world_to_base" type="fixed">
        <parent link="world"/>
        <child link="base"/>
        <origin xyz="0 0 1" rpy="0 3.1415 0"/>
    </joint>

    <joint name="joint_1" type="revolute">
        <parent link="base"/>
        <child link="link_1"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <limit effort="0" velocity="0"/>
        <axis xyz="0 1 0"/>
        <dynamics damping="1" friction="0" />
    </joint>

    <joint name="joint_2" type="revolute">
        <parent link="link_eef"/>
        <child link="link_2"/>
        <origin xyz="0 0 0.05" rpy="0 0 0"/>
        <limit effort="0" velocity="0"/>
        <axis xyz="0 1 0"/>
        <dynamics damping="1" friction="0" />
    </joint>

    <joint name="joint_3" type="revolute">
        <parent link="link_eef_2"/>
        <child link="link_3"/>
        <origin xyz="0 0 0.05" rpy="0 0 0"/>
        <limit effort="0" velocity="0"/>
        <axis xyz="0 1 0"/>
        <dynamics damping="1" friction="0" />
    </joint>

    <joint name="joint_eef" type="fixed">
        <parent link="link_1"/>
        <child link="link_eef"/>
        <origin xyz="0 0 0.8" rpy="0 0 0"/>
    </joint>

    <joint name="joint_eef_2" type="fixed">
        <parent link="link_2"/>
        <child link="link_eef_2"/>
        <origin xyz="0 0 0.8" rpy="0 0 0"/>
    </joint>

    <joint name="joint_eef_3" type="fixed">
        <parent link="link_3"/>
        <child link="link_eef_3"/>
        <origin xyz="0 0 0.8" rpy="0 0 0"/>
    </joint>



</robot>